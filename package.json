{
  "name": "lba2remake",
  "version": "0.6.5",
  "description": "Little Big Adventure 2 HD version for the web",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development node devserver.js",
    "dev:debug": "cross-env SRCMAP=true NODE_ENV=development node devserver.js",
    "dev:prod": "cross-env NODE_ENV=production node devserver.js",
    "build": "npm run build:bundle && npm run copy:bundle && npm run copy:webxr-assets && npm run build:index",
    "build:bundle": "cross-env NODE_ENV=production webpack --mode production",
    "build:bundle:debug": "cross-env SRCMAP=true NODE_ENV=development webpack",
    "build:index": "cross-env NODE_ENV=production node utils/build-index.js",
    "copy:webxr-assets": "rm -rf www/webxr-assets && cp -r ./node_modules/@webxr-input-profiles/assets/dist/profiles www/webxr-assets",
    "copy:bundle": "cp -r ./dist/bundle.js ./www/bundle.js",
    "clean": "rm -rf ./dist/* ./www/webxr-assets ./www/bundle.js ./www/index.html",
    "lint": "tslint -t msbuild -p ./tsconfig.json",
    "typecheck": "tsc",
    "import": "utils/convert/import.sh",
    "import:win": "powershell -File utils/convert/import.ps1",
    "convert": "ts-node utils/convert/convert.ts",
    "unpack": "ts-node utils/convert/unpack.ts"
  },
  "dependencies": {
    "@lbalab/hqr": "^3.4.4",
    "@types/dom-mediacapture-record": "^1.0.6",
    "@types/lodash": "^4.14.169",
    "@types/react-dom": "^16.9.13",
    "@types/three": "^0.140.0",
    "@webxr-input-profiles/assets": "^1.0.8",
    "@webxr-input-profiles/motion-controllers": "^1.0.0",
    "blockly": "^6.20210701.0",
    "color-convert": "^2.0.1",
    "file-saver": "^2.0.5",
    "form-data": "^4.0.0",
    "hash-wasm": "^4.9.0",
    "jest-diff": "^25.5.0",
    "loader-utils": "^3.2.1",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.7",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-markdown": "^6.0.2",
    "sass": "^1.43.4",
    "sass-loader": "^12.3.0",
    "simplex-noise": "^2.4.0",
    "three": "^0.151.0",
    "webxr-polyfill": "^2.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@babel/register": "^7.13.16",
    "@google-cloud/storage": "^5.8.5",
    "@netlify/functions": "^1.3.0",
    "@types/node": "^13.13.52",
    "acorn": "^7.4.1",
    "body-parser": "^1.19.0",
    "cross-env": "^5.2.1",
    "css-loader": "^5.2.4",
    "express": "^4.17.3",
    "ffmpeg-cli": "^2.7.3",
    "json-loader": "^0.5.7",
    "node-libs-browser": "^1.1.1",
    "raw-loader": "^0.5.1",
    "source-map-loader": "^0.2.4",
    "style-loader": "^2.0.0",
    "synth-js": "^0.1.2",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.10.2",
    "tslint": "^5.20.1",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-loader": "^3.6.0",
    "typescript": "^3.9.9",
    "webpack": "^5.76.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-middleware": "^7.2.0",
    "yml-loader": "^2.1.0"
  },
  "repository": "https://github.com/LBALab/lba2remake.git",
  "author": "Adrien Grandemange",
  "license": "MIT",
  "readme": "README.md"
}
