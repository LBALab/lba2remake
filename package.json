{
  "name": "lba2remake",
  "version": "0.5.0-alpha",
  "description": "Little Big Adventure 2 HD version for the web",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development node devserver.js",
    "dev:debug": "cross-env SRCMAP=true NODE_ENV=development node devserver.js",
    "dev:prod": "cross-env NODE_ENV=production node devserver.js",
    "build": "cross-env NODE_ENV=production webpack -p",
    "build:debug": "cross-env SRCMAP=true NODE_ENV=development webpack -p",
    "lint": "tslint -t msbuild -p ./tsconfig.json",
    "typecheck": "tsc",
    "import": "utils/convert/import.sh",
    "import:win": "powershell -File utils/convert/import.ps1",
    "convert": "ts-node utils/convert/convert.ts",
    "unpack": "ts-node utils/convert/unpack.ts"
  },
  "dependencies": {
    "@types/lodash": "^4.14.150",
    "@types/react-dom": "^16.9.7",
    "@types/three": "^0.127.0",
    "@webxr-input-profiles/assets": "^1.0.8",
    "@webxr-input-profiles/motion-controllers": "^1.0.0",
    "blockly": "^3.20200402.1",
    "color-convert": "^2.0.1",
    "file-saver": "^2.0.2",
    "jest-diff": "^25.5.0",
    "jszip": "^3.4.0",
    "lodash": "^4.17.21",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-markdown": "^4.3.1",
    "simplex-noise": "^2.4.0",
    "three": "^0.127.0",
    "webxr-polyfill": "^2.0.3",
    "xxhashjs": "^0.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/register": "^7.9.0",
    "@types/node": "^13.13.4",
    "acorn": "^7.1.1",
    "body-parser": "^1.19.0",
    "cross-env": "^5.2.1",
    "css-loader": "^2.1.1",
    "express": "^4.17.1",
    "ffmpeg-cli": "^2.7.1",
    "json-loader": "^0.5.7",
    "node-libs-browser": "^1.1.1",
    "node-sass": "^4.14.1",
    "raw-loader": "^0.5.1",
    "sass-loader": "^7.3.1",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "synth-js": "^0.1.2",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.9.1",
    "tslint": "^5.20.1",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-loader": "^3.6.0",
    "typescript": "^3.8.3",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2",
    "yml-loader": "^2.1.0",
    "@google-cloud/storage": "^5.3.0"
  },
  "repository": "https://github.com/agrande/lba2remake.git",
